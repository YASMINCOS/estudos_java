
public class ifswitct {
	public static void main(String[]args) {
		
		int num=15;
		
		if (num>10) {
			System.out.println("O número é maior que 10");
		}else if (num <10){
			System.out.println("O número não é maior que 10");
		}else {
			System.out.println("O númeroé igual a 10");

		}
	}

}
