import java.util.ArrayList;
import java.util.List;

public class Aplicacao {
	
	public static void main(String[] args) {
		
		//daimont
		List<String> l = new ArrayList<>();
		
		l.add("abc");
		l.add("def");
		//posicionar elemento
		//l.set(0, "ghi");
		l.add(0,"ghi");
		
		for ( String s: l) {
			System.out.println(s);
		}
		
	}

}
