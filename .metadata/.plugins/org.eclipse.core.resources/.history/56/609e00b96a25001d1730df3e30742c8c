
public class VariaveisAp {
	
	public static void main(String[]args) {
		
	    int numero =50;
	    int n2= numero+5;
	    
	    //numero= numero -1;
	    //numero -=1;
	    //numero --;
	    
	    //numero = numero *2;
	    numero *=2;
		
		System.out.println(numero);
		System.out.println(n2);

	}

}
